# Configurações do Azure OpenAI
AZURE_OPENAI_MODELS=https://your-openai-resource.openai.azure.com/
AZURE_DEFAULT_DEPLOYMENT_NAME=gpt-4
AZURE_OPENAI_EMBEDDING_MODEL_NAME=text-embedding-ada-002

# Configurações do Azure Key Vault
KEY_VAULT_URL=https://your-keyvault.vault.azure.net/

# Configurações do Azure AI Search (RAG)
AI_SEARCH_ENDPOINT=https://your-search-service.search.windows.net
AI_SEARCH_INDEX_NAME=your-index-name

# Configurações do Redis
REDIS_URL=redis://localhost:6379/0

# Configurações de Associações Usuário-Token (NOVO)
# REDIS_URL já configurado acima - usado tanto para jobs quanto para associações
# Opcionalmente, pode-se usar Redis separado para associações:
# USER_TOKEN_REDIS_URL=redis://localhost:6379/1

# Configurações de Autenticação (para futuras integrações)
# JWT_SECRET_KEY=your-jwt-secret-key
# JWT_ALGORITHM=HS256
# JWT_EXPIRATION_HOURS=24

# Configurações de Logging (opcional)
# LOG_LEVEL=INFO
# LOG_FORMAT=json